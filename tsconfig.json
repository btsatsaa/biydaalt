{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/Banner/index.tsx", "app/components/Courses/index.tsxs", "app/components/Mentor/index.js", "pages/page.js", "pages/api/submit.js", "app/components/submit/index.js", "app/components/yourComponent/index.js", "app/components/Detail/index.эыч", "app/components/Detail/index.js", "app/components/Ranking/myRate.js", "app/components/Ranking/starRating.js", "app/components/Search/index.js", "pages/infermatin.tsx"],
  "exclude": ["node_modules"]
}
